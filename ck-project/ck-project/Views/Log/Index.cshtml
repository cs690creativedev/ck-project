@model IEnumerable<ck_project.lead_log_file>


@Html.Partial("_AdminMenu")

<div class="container">

    @Html.Action("Search")
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.emp_username)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.table_name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.column_name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.action_name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.update_date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.prvious_value)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.new_value)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.lead.project_name)
            </th>
            <th></th>
        </tr>
        @if (Model != null)
        {
            foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.emp_username)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.table_name)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.column_name)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.action_name)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.update_date)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.prvious_value)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.new_value)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.lead.project_name)
                    </td>

                </tr>
            }
        }
    </table>
</div>