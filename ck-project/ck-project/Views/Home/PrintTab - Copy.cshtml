@{ 
    ViewBag.Title = "PrintTab";
}

<style>
    .column {
        padding-top: 1%;
    }
</style>

<div class="container">
    <div class="row top-buffer">
        <label class="col-md-4 text-center">FOR CUSTOMER</label>
        <label class="col-md-4 text-center">FOR INSTALLER</label>
        <label class="col-md-4 text-center">FOR INTERNAL</label>
        <div class="col-md-4 column" align="center">
            <input type="button" value="Proposal" onclick="location.href='@Url.Action("Convert", "Print", new { url = HttpContext.Current.Request.Url, id = ViewBag.leadNumber, str = "Proposal" })'" class="btn btn-default" id="printBtn" />
        </div>
        <div class="col-md-4 column" align="center">
            <input type="button" value="Installation Details" onclick="location.href='@Url.Action("Convert", "Print", new { url = HttpContext.Current.Request.Url, id = ViewBag.leadNumber, str = "InstallationDetailsForInstallers" })'" class="btn btn-default" id="printBtn" />
        </div>
        <div class="col-md-4 column" align="center">
            <input type="button" value="Lead Entry" onclick="location.href='@Url.Action("Convert", "Print", new { url = HttpContext.Current.Request.Url, id = ViewBag.leadNumber, str = "LeadEntry" })'" class="btn btn-default" id="printBtn" />
        </div>
        <div class="col-md-4 column" align="center">
            <input type="button" value="Products (Exhibit A)" onclick="location.href='@Url.Action("Convert", "Print", new { url = HttpContext.Current.Request.Url, id = ViewBag.leadNumber, str = "ProductsExternal" })'" class="btn btn-default" id="printBtn" />
        </div>
        <div class="col-md-4 col-md-offset-4 column" align="center">
            <input type="button" value="Products Ordering" onclick="location.href='@Url.Action("Convert", "Print", new { url = HttpContext.Current.Request.Url, id = ViewBag.leadNumber, str = "ProductsInternal" })'" class="btn btn-default" id="printBtn" />
        </div>
        <div class="col-md-4 column" align="center">
            <input type="button" value="Scope of Work (Exhibit B)" onclick="location.href='@Url.Action("Convert", "Print", new { url = HttpContext.Current.Request.Url, id = ViewBag.leadNumber, str = "ScopeOfWork" })'" class="btn btn-default" id="printBtn" />
        </div>
        <div class="col-md-4 col-md-offset-4 column" align="center">
            <input type="button" value="Installation Details" onclick="location.href='@Url.Action("Convert", "Print", new { url = HttpContext.Current.Request.Url, id = ViewBag.leadNumber, str = "InstallationDetailsInternal" })'" class="btn btn-default" id="printBtn" />
        </div>
        <div class="col-md-4 column" align="center">
            <input type="button" value="Owner Product Agreement" onclick="location.href='@Url.Action("Convert", "Print", new { url = HttpContext.Current.Request.Url, id = ViewBag.leadNumber, str = "OwnerProductAgreement" })'" class="btn btn-default" id="printBtn" />
        </div>
        <div class="col-md-4 col-md-offset-4 column" align="center">
            <input type="button" value="Sold Job Notice" onclick="location.href='@Url.Action("Convert", "Print", new { url = HttpContext.Current.Request.Url, id = ViewBag.leadNumber, str = "SoldJobNotice" })'" class="btn btn-default" id="printBtn" />
        </div>
    </div>        
</div>
